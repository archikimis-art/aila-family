0);\n    const savedTranslateX = (0, _reactNativeReanimated.useSharedValue)(0);\n    const savedTranslateY = (0, _reactNativeReanimated.useSharedValue)(0);\n    const MIN_SCALE = 0.3;\n    const MAX_SCALE = 3;\n\n    // Reset to center\n    const resetToCenter = (0, _react.useCallback)(function () {\n      const _e = [new global.Error(), -8, -27];\n      const treeTsx1 = function () {\n        scale.value = (0, _reactNativeReanimated.withSpring)(1, {\n          damping: 15\n        });\n        savedScale.value = 1;\n        translateX.value = (0, _reactNativeReanimated.withSpring)(0, {\n          damping: 15\n        });\n        translateY.value = (0, _reactNativeReanimated.withSpring)(0, {\n          damping: 15\n        });\n        savedTranslateX.value = 0;\n        savedTranslateY.value = 0;\n      };\n      treeTsx1.__closure = {\n        scale,\n        withSpring: _reactNativeReanimated.withSpring,\n        savedScale,\n        translateX,\n        translateY,\n        savedTranslateX,\n        savedTranslateY\n      };\n      treeTsx1.__workletHash = 4306488264475;\n      treeTsx1.__initData = _worklet_4306488264475_init_data;\n      treeTsx1.__stackDetails = _e;\n      return treeTsx1;\n    }(), []);\n\n    // Zoom in/out functions for buttons\n    const zoomIn = (0, _react.useCallback)(() => {\n      const newScale = Math.min(savedScale.value * 1.3, MAX_SCALE);\n      scale.value = (0, _reactNativeReanimated.withSpring)(newScale, {\n        damping: 15\n      });\n      savedScale.value = newScale;\n    }, []);\n    const zoomOut = (0, _react.useCallback)(() => {\n      const newScale = Math.max(savedScale.value / 1.3, MIN_SCALE);\n      scale.value = (0, _reactNativeReanimated.withSpring)(newScale, {\n        damping: 15\n      });\n      savedScale.value = newScale;\n    }, []);\n\n    // Pinch gesture for zoom\n    const pinchGesture = _reactNativeGestureHandler.Gesture.Pinch().onUpdate(function () {\n      const _e = [new global.Error(), -5, -27];\n