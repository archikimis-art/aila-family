wing birthday popup:', message);\n            if (Platform.default.OS === 'web') {\n              // Use a timeout to not block the UI\n              setTimeout(() => {\n                window.alert(message);\n              }, 1500);\n            } else {\n              Alert.default.alert('🎂 Anniversaire à venir', message);\n            }\n          }\n        }\n      } catch (error) {\n        console.log('Events not loaded:', error);\n      }\n    };\n    const toggleRecipient = email => {\n      setSelectedRecipients(prev => prev.includes(email) ? prev.filter(e => e !== email) : [...prev, email]);\n    };\n    const selectAllRecipients = () => {\n      const allEmails = collaborators.filter(c => c.status === 'accepted').map(c => c.email);\n      setSelectedRecipients(allEmails);\n    };\n    const handleCreateEvent = async () => {\n      if (!newEventTitle.trim()) return;\n      try {\n        await _srcServicesApi.eventsAPI.createEvent({\n          event_type: newEventType,\n          title: newEventTitle,\n          description: newEventDescription,\n          event_date: new Date().toISOString(),\n          recipients: selectedRecipients,\n          send_email: sendByEmail\n        });\n\n        // Reset form\n        setNewEventTitle('');\n        setNewEventDescription('');\n        setSelectedRecipients([]);\n        setShowCreateEvent(false);\n        loadEvents();\n\n        // Show success animation\n        setCurrentEvent({\n          type: newEventType,\n          title: `✅ Événement créé et envoyé!`,\n          subtitle: sendByEmail && selectedRecipients.length > 0 ? `Email envoyé à ${selectedRecipients.length} personne(s)` : 'Événement enregistré'\n        });\n        setShowEventsPanel(false);\n        setShowEventAnimation(true);\n      } catch (error) {\n        console.error('Failed to create event:', error);\n      }\n    };\n\n    // ============================================================================\n    // ZOOM & PAN - WEB SOLUT